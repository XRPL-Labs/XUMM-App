    "@types/react": "^17.0.1",
    "@types/react-native": "^0.63.48",
    "@types/react-test-renderer": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
