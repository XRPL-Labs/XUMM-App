    "@veriff/react-native-sdk": "2.3.2",
    "bignumber.js": "9.0.1",
    "fuse.js": "6.4.6",
    "i18n-js": "3.8.0",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.33",
